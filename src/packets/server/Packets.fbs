namespace SPackets;

table S00PacketKeepAlive
{
}

table S01PacketAuthenticate
{
	uid: string;
}

table S02PacketIRC
{
	author_role_id: ubyte;
	author_name: string;

	content: string;

	is_tell: bool;

	to: string;
}

table S03PacketCheck
{
	names: string;
}

union Packets { S00PacketKeepAlive, S01PacketAuthenticate, S02PacketIRC, S03PacketCheck }

table SPacket
{
	packets: Packets;
}

root_type SPacket;