namespace CPackets;

table C00PacketKeepAlive
{
	name: string;
}

table C01PacketAuthenticate
{
	uname: string;
	upass: string;
}

table C02PacketIRC
{
	content: string;
	tell: string;
	is_tell: bool;
}

table C03PacketCheck
{
	names: string;
}

union Packets { C00PacketKeepAlive, C01PacketAuthenticate, C02PacketIRC, C03PacketCheck }

table CPacket
{
	packets: Packets;
}

root_type CPacket;